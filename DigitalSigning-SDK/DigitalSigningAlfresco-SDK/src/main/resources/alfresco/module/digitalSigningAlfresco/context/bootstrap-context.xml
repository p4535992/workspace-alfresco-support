<?xml version='1.0' encoding='UTF-8'?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
          http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <!-- The bootstrap-context.xml file is used for patch definitions, importers, 
		 workflow, and loading custom content models.  -->

    <!-- load the custom content model -->
    <bean id="digitalSigning-model" parent="dictionaryModelBootstrap" depends-on="dictionaryBootstrap">
		<property name="models">
			<list>
				<value>alfresco/module/${project.artifactId}/model/digitalSigningModel.xml</value>
			</list>
		</property>
		<property name="labels">
            <list>
                <value>alfresco/module/${project.artifactId}/messages/signingContentModel-messages</value>
            </list>
        </property>
    </bean>

</beans>
