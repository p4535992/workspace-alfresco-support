<?xml version="1.0" encoding="UTF-8"?>
<model xmlns="http://www.alfresco.org/model/dictionary/1.0" name="sc:somecomodel">
    <description>Someco Model</description>
    <author>Optaros</author>
    <version>1.0</version>
    <imports>
        <import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d"/>
        <import uri="http://www.alfresco.org/model/content/1.0" prefix="cm"/>
        <import uri="http://www.alfresco.org/model/system/1.0" prefix="sys"/>
    </imports>
    <namespaces>
        <namespace uri="http://www.someco.com/model/content/1.0" prefix="sc"/>
    </namespaces>
    <data-types/>
    <constraints>
        <constraint name="sc:campaignList" type="LIST">
            <parameter name="allowedValues">
                <list>
                    <value>Application Syndication</value>
                    <value>Private Event Retailing</value>
                    <value>Social Shopping</value>
                </list>
            </parameter>
        </constraint>
    </constraints>
    <types>
        <type name="sc:doc">
            <title>Someco Document</title>
            <parent>cm:content</parent>
            <properties/>
            <associations>
                <association name="sc:relatedDocuments">
                    <title>Related Documents</title>
                    <source>
                        <mandatory>false</mandatory>
                        <many>true</many>
                    </source>
                    <target>
                        <class>sc:doc</class>
                        <mandatory enforced="false">false</mandatory>
                        <many>true</many>
                    </target>
                </association>
            </associations>
            <overrides/>
            <mandatory-aspects>
                <aspect>cm:generalclassifiable</aspect>
            </mandatory-aspects>
        </type>
        <type name="sc:marketingDoc">
            <title>Someco Marketing Document</title>
            <parent>sc:doc</parent>
            <properties>
                <property name="sc:campaign">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <multiple>true</multiple>
                    <index enabled="true"/>
                    <constraints>
                        <constraint name="sc:somecomodel_marketingDoc_campaign_anon_0" ref="sc:campaignList"/>
                    </constraints>
                </property>
            </properties>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </type>
        <type name="sc:whitepaper">
            <title>Someco Whitepaper</title>
            <parent>sc:marketingDoc</parent>
            <properties/>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </type>
        <type name="sc:hrDoc">
            <title>Someco HR Document</title>
            <parent>sc:doc</parent>
            <properties/>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </type>
        <type name="sc:salesDoc">
            <title>Someco Sales Document</title>
            <parent>sc:doc</parent>
            <properties/>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </type>
        <type name="sc:opsDoc">
            <title>Someco Operations Document</title>
            <parent>sc:doc</parent>
            <properties/>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </type>
        <type name="sc:legalDoc">
            <title>Someco Legal Document</title>
            <parent>sc:doc</parent>
            <properties/>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </type>
        <type name="sc:hrPolicy">
            <title>Someco HR Policy</title>
            <parent>sc:hrDoc</parent>
            <properties/>
            <associations/>
            <overrides/>
            <mandatory-aspects>
                <aspect>cm:replaceable</aspect>
            </mandatory-aspects>
        </type>
        <type name="sc:rating">
            <title>Someco Rating</title>
            <parent>sys:base</parent>
            <properties>
                <property name="sc:rating">
                    <type>d:int</type>
                    <mandatory>true</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="sc:rater">
                    <type>d:text</type>
                    <mandatory>true</mandatory>
                    <index enabled="true"/>
                </property>
            </properties>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </type>
    </types>
    <aspects>
        <aspect name="sc:webable">
            <title>SomecoWebable</title>
            <properties>
                <property name="sc:published">
                    <type>d:date</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="sc:isActive">
                    <type>d:boolean</type>
                    <mandatory>false</mandatory>
                    <default>false</default>
                    <index enabled="true"/>
                </property>
            </properties>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </aspect>
        <aspect name="sc:clientRelated">
            <title>Someco Client Metadata</title>
            <properties>
                <property name="sc:clientName">
                    <type>d:text</type>
                    <mandatory>true</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="sc:projectName">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
            </properties>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </aspect>
        <aspect name="sc:rateable">
            <title>Someco Rateable</title>
            <properties>
                <property name="sc:averageRating">
                    <type>d:double</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="sc:totalRating">
                    <type>d:int</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="sc:ratingCount">
                    <type>d:int</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
            </properties>
            <associations>
                <child-association name="sc:ratings">
                    <title>Rating</title>
                    <source>
                        <mandatory>false</mandatory>
                        <many>true</many>
                    </source>
                    <target>
                        <class>sc:rating</class>
                        <mandatory enforced="false">false</mandatory>
                        <many>true</many>
                    </target>
                </child-association>
            </associations>
            <overrides/>
            <mandatory-aspects/>
        </aspect>
    </aspects>
</model>