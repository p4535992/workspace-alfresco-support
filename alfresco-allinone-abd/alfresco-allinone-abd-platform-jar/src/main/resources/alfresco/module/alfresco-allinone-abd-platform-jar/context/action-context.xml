<?xml version='1.0' encoding='UTF-8'?> 
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
          http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
          
    <bean id="set-web-flag" 
        class="it.abd.alfresco_allinone_abd.actions.SetWebFlag" 
        parent="action-executer"> 

        <property name="nodeService" ref="NodeService" /> 
        <property name="publicAction">
            <value>false</value>
        </property> 
    </bean> 
        
    <bean id="someco.actionResourceBundles" class="org.alfresco.i18n.ResourceBundleBootstrapComponent">
		<property name="resourceBundles">
			<list>
				<value>alfresco.module.${project.artifactId}.messages.action-config</value>
			</list>
		</property>
	</bean>
	
    <bean id="move-replaced" 
        class="it.abd.alfresco_allinone_abd.actions.MoveReplacedActionExecuter"  
        parent="action-executer"> 
            <property name="nodeService" ref="NodeService" /> 
            <property name="fileFolderService" ref="FileFolderService" /> 
    </bean> 
    
	
</beans> 
